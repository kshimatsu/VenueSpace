<div class="main-container">
    <div class="row">

      <div class="main-container-body">
        <% @venues.each do |venue| %>
        <div class="venue-container">
          <div class="text-container">
            <a href="/venues/<%= venue.id %>"> <!-- How to do with link_to? -->
              <%= venue.name %>
              <p><%= venue.id %></p>
              <p><%= venue.city %></p>
              <p><%= venue.state %></p>
              <p><%= venue.budget %></p>
            </a>
          </div>
        </div>
        <% end %>
      </div>




